<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="48dp"
                android:height="48dp" />
            <solid android:color="#000000" />
            <corners android:radius="8dp"/>
        </shape>
    </item>

    <item
        android:bottom="8dp"
        android:left="20dp"
        android:right="20dp"
        android:top="8dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#ffffff" />
                <corners android:radius="1dp" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="20dp"
        android:left="8dp"
        android:right="8dp"
        android:top="20dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#ffffff" />
                <corners android:radius="1dp" />
            </shape>
        </rotate>
    </item>
</layer-list>
